{
  "name": "@dev-agency/vcs-integration",
  "version": "1.0.0",
  "description": "GitHub/GitLab integration agent with PR management, issue sync, CI/CD monitoring, and conflict resolution",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "github",
    "gitlab",
    "vcs",
    "integration",
    "pr-automation",
    "ci-cd",
    "conflict-resolution",
    "dev-agency"
  ],
  "author": "Dev-Agency Team",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "@octokit/webhooks": "^12.0.10",
    "@gitbeaker/rest": "^39.21.0",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "axios": "^1.6.0",
    "rate-limiter-flexible": "^4.0.1",
    "crypto": "^1.0.1",
    "diff3": "^0.0.3",
    "simple-git": "^3.20.0",
    "mustache": "^4.2.0",
    "yaml": "^2.3.4",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.10",
    "@types/mustache": "^4.2.5",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dev-agency/vcs-integration.git"
  },
  "bugs": {
    "url": "https://github.com/dev-agency/vcs-integration/issues"
  },
  "homepage": "https://github.com/dev-agency/vcs-integration#readme"
}